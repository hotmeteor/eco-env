{
  "name": "hotmeteor/eco-env",
  "description": ".env file writer",
  "type": "project",
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@hotmeteor.com"
    }
  ],
  "bin": [
    "eco"
  ],
  "require": {
    "php": "^7.2",
    "vlucas/phpdotenv": "^3.0|^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Eco\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eco\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
